{
  "name": "@repo/effect",
  "private": true,
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/src/index.d.ts",
      "default": "./src/index.ts"
    },
    "./db": {
      "types": "./dist/src/db.d.ts",
      "default": "./src/db.ts"
    },
    "./errors": {
      "types": "./dist/src/errors.d.ts",
      "default": "./src/errors.ts"
    },
    "./runtime": {
      "types": "./dist/src/runtime.d.ts",
      "default": "./src/runtime.ts"
    },
    "./logging": {
      "types": "./dist/src/logging.d.ts",
      "default": "./src/logging.ts"
    },
    "./telemetry": {
      "types": "./dist/src/telemetry.d.ts",
      "default": "./src/telemetry.ts"
    }
  },
  "scripts": {
    "build": "tsc --build",
    "clean": "git clean -xdf .cache .turbo dist node_modules",
    "dev": "tsc --build tsconfig.package.json",
    "format": "prettier --check . --ignore-path ../../.gitignore",
    "lint": "eslint",
    "test": "NODE_OPTIONS='--experimental-vm-modules' jest",
    "test:watch": "NODE_OPTIONS='--experimental-vm-modules' jest --watch",
    "typecheck": "tsc --build --noEmit --emitDeclarationOnly false"
  },
  "prettier": "@repo/prettier-config",
  "dependencies": {
    "@repo/db": "workspace:*",
    "@opentelemetry/api": "catalog:",
    "@opentelemetry/resources": "catalog:",
    "@opentelemetry/sdk-trace-base": "catalog:",
    "@opentelemetry/sdk-trace-node": "catalog:",
    "@opentelemetry/semantic-conventions": "catalog:",
    "effect": "^3.16.0"
  },
  "devDependencies": {
    "@repo/eslint-config": "workspace:*",
    "@repo/jest-config": "workspace:*",
    "@repo/prettier-config": "workspace:*",
    "@repo/typescript-config": "workspace:*",
    "@types/node": "catalog:",
    "eslint": "catalog:",
    "prettier": "catalog:",
    "typescript": "catalog:",
    "jest": "catalog:",
    "@types/jest": "catalog:",
    "ts-jest": "catalog:"
  }
}
